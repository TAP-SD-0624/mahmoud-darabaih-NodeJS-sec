<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>File List</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1>File List</h1>
    <ul>
      <% files.forEach(file => { %>
      <li><a href="/files/<%= file %>"><%= file %></a></li>
      <% }) %>
    </ul>
    <a href="/create">Create New File</a>
  </body>
</html>
